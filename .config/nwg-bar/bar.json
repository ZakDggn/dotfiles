[
  {
    "label": "_Lock",
    "exec": "swaylock -f -c 000000",
    "icon": "/usr/share/wlogout/icons/lock.png"
  },
  {
    "label": "_Suspend",
    "exec": "systemctl suspend",
    "icon": "/usr/share/wlogout/icons/suspend.png"
  },
  {
    "label": "_Exit session)",
    "exec": "swaymsg exit",
    "icon": "/usr/share/wlogout/icons/logout.png"
  },
  {
    "label": "_Reboot",
    "exec": "systemctl reboot",
    "icon": "/usr/share/wlogout/icons/reboot.png"
  },
  {
    "label": "_Poweroff",
    "exec": "systemctl -i poweroff",
    "icon": "/usr/share/wlogout/icons/shutdown.png"
  }
]
